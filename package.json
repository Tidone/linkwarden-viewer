{
    "name": "web-extension-boilerplate-23",
    "longName": "Manage LinkWarden Bookmarks",
    "description": "Manage Linkwarden Bookmarks",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "format": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "start": "concurrently \"npm run dev\" \"npm run build\"",
        "tailwind": "tailwindcss -i src/styles/index.css -o dist/assets/index.css",
        "dev": "vite",
        "build": "tsc && vite build && npm run build:js && npm run build:zip",
        "build:pre": "esno scripts/prebuild.ts",
        "build:post": "esno scripts/postbuild.ts",
        "build:js": "vite build --config vite.config.content.ts",
        "build:zip": "cd dist && zip -r -FS ../extension.zip *",
        "preview": "vite preview"
    },
    "author": "Doug Silkstone <doug@withseismic.com>",
    "license": "ISC",
    "devDependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/material": "^7.1.0",
        "@types/chrome": "^0.0.240",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.4.1",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/webextension-polyfill": "^0.10.1",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "esno": "^0.16.3",
        "fs-extra": "^11.1.1",
        "postcss": "^8.4.25",
        "prettier": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-spinners": "^0.17.0",
        "react-select": "^5.10.1",
        "tailwindcss": "^3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "unplugin-auto-import": "^0.16.6",
        "vite": "^4.4.3",
        "vite-plugin-static-copy": "^0.16.0",
        "webext-bridge": "^6.0.1",
        "webextension-polyfill": "^0.10.0",
        "webextension-polyfill-ts": "^0.26.0"
    },
    "dependencies": {
        "lucide-react": "^0.451.0"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
